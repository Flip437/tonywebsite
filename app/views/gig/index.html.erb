<!-- ##### Blog Area Start ##### -->
<div class="blog-area section-padding-100">
    <div class="container">
        <div class="row">
            <div class="col-12 col-lg-9">

                <% @gigs.each do |gig| %>            
                    <!-- Single Post Start -->
                    <div class="single-blog-post mb-100 wow fadeInUp" data-wow-delay="100ms">
                        <!-- Post Thumb -->
                        <div class="blog-post-thumb mt-30">
                        <br><br>
                            <!-- Post Date -->
                            <div class="post-date">
                                <span> <%= gig.day %> </span>
                                <span> <%=gig.month%> ‘<%=gig.year%> </span>
                            </div>
                        </div>

                        <!-- Blog Content -->
                        <div class="blog-content">
                            <!-- Post Title -->
                            <%=link_to gig.title, gig_path(gig), class:"post-title"%>
                            <!-- Post Meta -->
                            <div class="post-meta d-flex mb-30">
                                <p class="post-author">By<a href="#"> Admin</a></p>
                                <p class="tags">in<a href="#"> Events</a></p>
                                <p class="tags"><a href="#">2 Comments</a></p>
                            </div>
                            <!-- Post Excerpt -->
                            <p> <%=gig.description%> </p>
                        </div>
                    </div>
                <% end %>

                <!-- Single Post Start -->
                <div class="single-blog-post mb-100 wow fadeInUp" data-wow-delay="100ms">
                    <!-- Post Thumb -->
                    <div class="blog-post-thumb mt-30">
                    <br><br>
                        <a href="#"><img src="img/bg-img/blog2.jpg" alt=""></a>
                        <!-- Post Date -->
                        <div class="post-date">
                            <span>15</span>
                            <span>June ‘18</span>
                        </div>
                    </div>

                    <!-- Blog Content -->
                    <div class="blog-content">
                        <!-- Post Title -->
